package com.j2.templatemethod.sort;

import java.util.Comparator;
public class DuckComparator implements Comparator<Duck>{
  String name;
  int weight;
  
  public DuckComparator(String name, int weight) {
  this.name = name;
  this.weight = weight;
 }
  public String getName() {
    return name;
  }
  public int getWeight() {
    return weight;
  }
 
 public String toString() {
  return name + " weighs " + weight;
 }

 
 public int compare(Object d1, Object d2) {
    int duckWeight1 = d1.getWeight();
    int duckWeight2 = d2.getWeight();
    return duckWeight1.CompareTo(duckWeight2);

   
   
}
}
   
  

 
 
  
 